{
  "name": "miletracker",
  "version": "0.1.0",
  "description": "A mileage tracker for calculating tax reimbursements",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node bin/dev",
    "server": "nodemon bin/dev",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bladecla/mileageTracker.git"
  },
  "author": "Blake Clark",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bladecla/mileageTracker/issues"
  },
  "homepage": "https://github.com/bladecla/mileageTracker#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.13"
  },
  "devDependencies": {
    "dotenv": "^6.1.0",
    "nodemon": "^1.18.6"
  }
}
